{
  "name": "remix-template-express",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run prebuild:css && run-p build:*",
    "build:dom-scripts": "esbuild ./app/DOM/utils.ts --entry-names=[name]-[hash] --outdir=./public/utils",
    "build:remix": "remix build",
    "prebuild:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "remix build && run-p dev:*",
    "dev:node": "env-cmd -f .env.dev tsx watch ./server.ts",
    "dev:remix": "remix watch",
    "dev-with-hotreload": "run-p remix-dev dev:css dev:dom-scripts",
    "remix-dev": "env-cmd -f .env.dev remix dev",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:dom-scripts": "esbuild ./app/DOM/utils.ts --entry-names=[name]-[hash] --outdir=./public/utils --watch",
    "start": "run-p start:*",
    "start:server": "cross-env NODE_ENV=production tsx ./server.ts",
    "start:cursor-chat": "cross-env HOST=localhost PORT=9002 y-websocket"
  },
  "dependencies": {
    "@metronome-sh/express": "^5.5.3",
    "@metronome-sh/react": "^5.5.3",
    "@remix-run/express": "^1.5.1",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "common-tags": "^1.8.2",
    "compression": "^1.7.4",
    "copy-to-clipboard": "^3.3.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "express": "^4.18.1",
    "getenv.ts": "^1.4.0",
    "http-proxy": "^1.18.1",
    "lmdb": "^2.4.2",
    "lodash-es": "^4.17.21",
    "morgan": "^1.10.0",
    "msgpackr": "^1.6.1",
    "npm-run-all": "^4.1.5",
    "ordered-binary": "^1.2.5",
    "proxy-from-env": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-toastify": "^9.0.3",
    "remix-utils": "^3.2.0",
    "short-uuid": "^4.2.0",
    "simple-node-logger": "^21.8.12",
    "tiny-invariant": "^1.2.0",
    "ts-brand": "^0.0.2",
    "tsx": "^3.4.2",
    "use-debounce": "^8.0.1",
    "y-websocket": "^1.4.3",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "@types/common-tags": "^1.8.1",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash-es": "^4.17.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.40",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/ws": "^8.5.3",
    "autoprefixer": "^10.4.7",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.14.42",
    "eslint": "^8.15.0",
    "postcss": "^8.4.14",
    "sha1-file": "^3.0.0",
    "shiki": "^0.10.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
